{
  "functions": {
    "api/index.ts": {
      "runtime": "@vercel/node",
      "memory": 1024,
      "maxDuration": 30
    }
  },

  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/index.ts"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
